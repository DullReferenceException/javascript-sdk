{
  "name": "@optimizely/optimizely-datafile-manager",
  "version": "1.0.0",
  "description": "Datafile manager package for Optimizely X Full Stack",
  "main": "dist/common/index.js",
  "browser": "dist/index.browser.cjs.js",
  "module": "dist/es/index.js",
  "scripts": {
    "build": "npm run build-commonjs && npm run build-es && npm run build-rollup",
    "build-commonjs": "babel lib -d dist/common --presets=env,stage-3 --ignore __tests__",
    "build-es": "babel lib -d dist/es --presets=stage-3 --ignore __tests__",
    "build-rollup": "rollup -c rollup.config.js",
    "test": "NODE_ENV=test mocha --require babel-polyfill --require babel-register './lib/**/*.tests.js'"
  },
  "dependencies": {
    "@optimizely/optimizely-network-client": "1.0.0",
    "@optimizely/optimizely-sdk-core": "2.0.0-beta1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "mocha": "^5.0.4",
    "rollup": "^0.57.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "LICENSE",
    "CHANGELOG",
    "README.md"
  ]
}
